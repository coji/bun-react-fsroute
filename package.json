{
  "name": "my-bun-app",
  "version": "0.0.1",
  "author": "coji",
  "type": "module",
  "scripts": {
    "setup": "bun setup.ts",
    "start": "bun server.ts",
    "build": "run-p build:*",
    "build:css": "bun tailwind -i src/global.css -o src/public/global.css",
    "dev": "run-p dev:*",
    "dev:bun": "bun --hot server.ts",
    "dev:css": "bun tailwind --watch -i src/global.css -o src/public/global.css"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.279.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^3.4.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@types/web": "^0.0.115",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "autoprefixer": "^10.4.16",
    "bun-types": "^1.0.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.30",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
    ]
  }
}
